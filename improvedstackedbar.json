{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/RogueTrader7/Homework10/main/normalisedtree.csv"
    },
    
    "params": [
        {
            "name": "Population_Above",
            "value": 0,
            "bind": {
            "input": "range",
            "min": 4,
            "max": 35,
            "step": 0.1,
            "name": "Range of proportion: "
          }
        },
 {
 "name": "State_selection",
 "bind": {
 "input": "select",
 "options": [
 null,
 "NSW",
 "Vic",
 "Qld",
 "SA",
 "WA",
 "Tas",
 "NT",
 "ACT"
 ],
 "labels":[
 "Show All",
 "NSW",
 "Vic",
 "Qld",
 "SA",
 "WA",
 "Tas",
 "NT",
 "ACT"
 ],
 "name": "State Selection: "
 }
 }
 ],
 "transform": [
     {"filter": "datum.people > 0"},
     {"filter": "datum.people > 0"},
     {"filter": "datum.people > Population_Above"}
     ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "State",
        "type": "ordinal",
        "title": "Regions"
      },
      "y": {
        "aggregate": "max",
        "field": "people"
      },
      "color": {
        "type": "nominal",
        "field": "health status",
        "scale": {
          "domain": [
            "Excellent",
            "Very good",
            "Excellent / Very good",
            "Good",
            "Fair",
            "Fair / Poor"
          ],
          "range": [
            "#d73027",
            "#fc8d59",
            "#ffffbf",
            "#e0f3f8",
            "#91bfdb",
            "#4575b4"
          ]
        },
        "title": "Health Status"        
      },
       
      "tooltip": [
        {"field": "State", "type": "nominal"},
        {"field": "people", "type": "quantitative", "format": ","}
        ]
       
      }
  }
